{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdb",
      "request": "attach",
      "name": "debug FOS",
      "remote": true,
      "target": ":26000",
      "cwd": "${workspaceRoot}",
      "executable": "obj/kern/kernel",
      "preLaunchTask": "build-debug"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/fos_cygwin/FOS_CODES/FOS_PROJECT_2023_Template/lib",
      "program": "c:/fos_cygwin/FOS_CODES/FOS_PROJECT_2023_Template/lib/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}